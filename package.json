{
  "name": "cypress-template",
  "version": "1.0.0",
  "description": "Demo of Github actions with cypress + cucumber",
  "main": "index.js",
  "scripts": {
    "test-ui": "cypress open --browser chrome",
    "test": "cypress run --browser chrome",
    "make-steps": "node _auto-generate-step-definitions.js",
    "start": "node server.js"
  },
  "cypress-cucumber-preprocessor": {
    "json": {
      "enabled": true
    },
    "stepDefinitions": [
      "/specs/step_definitions/**/*.js"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "12.2",
    "@cypress/webpack-preprocessor": "5.12",
    "cypress": "10.7"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanishah30/lumia-cypress-tester-new.git"
  },
  "bugs": {
    "url": "https://github.com/vanishah30/lumia-cypress-tester-new/issues"
  },
  "homepage": "https://github.com/vanishah30/lumia-cypress-tester-new#readme"
}
